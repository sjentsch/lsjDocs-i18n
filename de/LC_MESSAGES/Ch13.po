msgid "1.2"
msgstr "1.2"

#: ../../Ch13/Ch13_ANOVA_06.rst:332
msgid "1.3"
msgstr "1.3"

#: ../../Ch13/Ch13_ANOVA_06.rst:332
msgid "1.4"
msgstr "1.4"

#: ../../Ch13/Ch13_ANOVA_06.rst:332
